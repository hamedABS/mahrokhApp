{
  "expo": {
    "name": "Mahrokh App",
    "slug": "MahrokhApp",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/png/logo.png",
    "splash": {
      "image": "./assets/png/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vista.mahrokhUserApp",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "comgooglemaps"
        ]
      }
    },
    "android": {
      "package": "com.vista.mahrokhUserApp",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "com.android.launcher.permission.INSTALL_SHORTCUT"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCR0aZyRvLOA4qy6iMXxFxwkO5D7UloMh4"
        }
      }
    },
    "description": ""
  }
}
